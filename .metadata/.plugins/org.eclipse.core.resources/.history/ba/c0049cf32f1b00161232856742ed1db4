package lrh.test;

import java.util.ArrayList ;
import java.util.List ;

public class Test
{
	public static void main ( String [ ] args )
	{
		List<Book> books = new ArrayList<Book>();
		books.add ( new Book(101,"C",45 , 5) );
		books.add ( new Book(102,"C#",45 , 5) );
		books.add ( new Book(103,"Java",45 , 5) );
		books.add ( new Book(104,"C++",45 , 5) );
		
		BookSystem system = new BookSystem(books);
		
		system.showBookInfo ( );
		
		system.shopping ( );
		
		system.showInfo ( );
		
	}
}
